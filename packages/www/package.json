		"ioredis": "^5.4.2",
		"mode-watcher": "^0.5.0",
		"open-props": "^1.7.7",
		"rehype-slug": "^6.0.0",
		"remark-toc": "^9.0.0",
		"resend": "^4.1.2",
		"shiki": "^1.23.1",
		"svelte-inview": "^4.0.4",
		"svelte-motion": "^0.12.2",
		"unique-names-generator": "^4.7.1",
		"uuid": "^11.0.5"
	},
	"cypress": {
		"reporter": "cypress-mochawesome-reporter",
		"reporterOptions": {
			"reportDir": "reports/cypress",
